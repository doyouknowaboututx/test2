				Title.TextYAlignment = Enum.TextYAlignment.Center
				Title.TextColor3 = Color3.fromRGB(255, 255, 255)
				Title.Text = isChloe and "Chloe Official Discord" or "UgPhone Android CloudPhone"
				Title.Parent = Panel

				local Content = Instance.new("TextLabel")
				Content.BackgroundTransparency = 1
				Content.Position = UDim2.new(0, 70, 0, 36)
				Content.Size = UDim2.new(1, -80, 0, 20)
				Content.Font = Enum.Font.MontserratBold
				Content.TextSize = 15
				Content.TextWrapped = true
				Content.TextXAlignment = Enum.TextXAlignment.Left
				Content.TextYAlignment = Enum.TextYAlignment.Top
				Content.TextColor3 = Color3.fromRGB(255, 255, 255)
				Content.Text = isChloe and "Chloe best Utility for AFK 24/7"
					or "24/7 AFK, Stable, Smooth For Roblox Farming"
				Content.Parent = Panel

				local Padding = 12
				local Gap = 8

				if isChloe then
					local Button = Instance.new("TextButton")
