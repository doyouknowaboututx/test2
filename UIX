					DiscordButton.Parent = Panel
					Instance.new("UICorner", DiscordButton).CornerRadius = UDim.new(0, 8)

					DiscordButton.MouseButton1Click:Connect(function()
						setclipboard("https://discord.com/invite/ugphone-official")
						chloex("Ugphone Discord link copied!")
					end)

					local LinkButton = Instance.new("TextButton")
					LinkButton.Size = UDim2.new(0.5, -Padding - Gap / 2, 0, 30)
					LinkButton.Position = UDim2.new(0.5, Gap / 2, 1, -45)
					LinkButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					LinkButton.BackgroundTransparency = 0.85
					LinkButton.Text = "Link Ugphone"
					LinkButton.Font = Enum.Font.MontserratBold
