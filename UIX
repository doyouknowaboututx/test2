					Button.Position = UDim2.new(0, Padding, 1, -45)
					Button.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					Button.BackgroundTransparency = 0.85
					Button.Text = "Copy Discord"
					Button.Font = Enum.Font.MontserratBold
					Button.TextSize = 13
					Button.TextColor3 = Color3.fromRGB(255, 255, 255)
					Button.Parent = Panel
					Instance.new("UICorner", Button).CornerRadius = UDim.new(0, 8)

					Button.MouseButton1Click:Connect(function()
						setclipboard("https://discord.gg/chloex")
						chloex("Chloe Discord link copied to clipboard!")
					end)
				else
					local DiscordButton = Instance.new("TextButton")
					DiscordButton.Size = UDim2.new(0.5, -Padding - Gap / 2, 0, 30)
					DiscordButton.Position = UDim2.new(0, Padding, 1, -45)
